{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/compat/(.*)",
      "dest": "/api/backend/compat/$1"
    },
    {
      "src": "/api/chat",
      "dest": "/api/ai/chat"
    },
    {
      "src": "/api/health",
      "dest": "/api/backend/health"
    }
  ]
}

